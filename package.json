{
  "name": "mistral11",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd react2 && npm run dev",
    "build": "cd react2 && npm install && npm run build && cd ../backend && npm install && npm run build",
    "vercel-build": "cd react2 && npm install --include=dev && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../react2 && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "engines": {
    "node": "22.x"
  }
}